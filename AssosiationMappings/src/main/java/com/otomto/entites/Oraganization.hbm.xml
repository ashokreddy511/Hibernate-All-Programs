<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.otomto.entites">
<class name="Organization" table="ORGANIZATION">
	<id name="organizationNo" column="ORGANIZATION_NO">
	<generator class="increment"></generator>
	</id>
	<property name="organizationName" column="ORGANIZATION_NAME"/>
	<property name="description" column="DESCRIPTION"/>
	<property name="establishingDate" column="ESTABLISHINGDATE"/>
	<property name="businessType" column="BUSINESSTYPE"/>
</class>
</hibernate-mapping>